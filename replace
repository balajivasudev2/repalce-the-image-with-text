			<div id='cc'></div>
			<div id='gg'></div>
			<script>
				var text=$('#bb').html().split(',,');
				var text1=[];
				for(i=0;i<text.length;i++)
				{
					text1.push('<span class="ab">'+text[i]+'</span>')
				}				
$('#cc').html(text1)
$('#aa').click(function(){
var id=$(this).html();
$('.ab').click(function(){
$(this).html(id);
}); });
</script>
